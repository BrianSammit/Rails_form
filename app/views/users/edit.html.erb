</h1>Edit an user </>
<% 
=begin %>
 <form action="/users" method="post" accept-charset="UTF-8">
  <label for="username">Username:</label>
  <input type="text" id="username" name="user[username]"><br><br>
  <label for="email">Email:</label>
  <input type="email" id="email" name="user[email]"><br><br>
  <label for="password">Password:</label>
  <input type="password" id="password" name="user[password]"><br><br>
  <input type="submit" value="Submit">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
</form> 
<% 
=end %>

<% 
=begin %>
 <%= form_tag('/users', method: :post) do %> 
   <%= label_tag(:username, "Username:") %> 
   <%= text_field_tag(:username) %> 
   <%= label_tag(:email, "Email:") %> 
   <%= email_field_tag(:email) %> 
   <%= label_tag(:password, "Password:") %> 
   <%= password_field_tag(:password) %>    
   <%= submit_tag("Create") %> 
  <% end %>

  <% 
=end %>
  

  <%= form_for @user do |f| %>
  Username: <%= f.text_field :username %><br />
  Email : <%= f.email_field :email%><br />
  Password : <%= f.password_field :password %><br />
  <%= f.submit %>
<% end %>
